{
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "arrowParens": "always",
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": true,
    "plugins": [
        "prettier-plugin-blade",
        "prettier-plugin-tailwindcss",
        "prettier-plugin-organize-imports"
    ],
    "pluginSearchDirs": false
}
